<h3><%= link_to @band.band_name, bands_path %><h3>


	Albums<br>
	<br>
	<% @band.albums.each do |album| %>
	<%= link_to album.album_name,
	band_album_path(album.band_id, album.id) %><br>

	<% album.tracks.each do |track| %>
	<%= link_to track.track_name,
	band_album_track_path(album.band_id, album.id, track.id) %><br>



	<% end %><br>
	<% end %>